<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Udlejning | Byggemarked A/S</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <header>
        <div class="logo">
            <a href="/">Udlejning - Byggemarked A/S</a>
        </div>
    </header>

    <main>
        @RenderBody()
    </main>

    <footer>
        <p>Copyright &copy; @DateTime.Now.Year. All right reserved by Andreas Fenger Østergaard</p>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>